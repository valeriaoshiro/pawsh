<div class="container">
<h1>Edit <%= @user.name %>'s profile</h1>
<%= form_for @user do |f| %>
    <div class="input-field">
        <%= f.text_field :name %>
        <%= f.label :name, class: "active" %>
    </div>
    
    <%= f.submit "Update profile!" %>
<% end %>

<%= link_to "Back", user_path(@user) %>
</div>

